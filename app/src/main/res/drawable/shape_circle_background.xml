<selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <size
                android:width="64dp"
                android:height="64dp"/>
            <solid android:color="#88E71C61"></solid>
            <corners android:radius="32dp"/>
        </shape>

    </item>
    <item>

        <shape
            android:shape="rectangle">
            <size
                android:width="64dp"
                android:height="64dp"/>
            <solid android:color="#FFE71C61"></solid>
            <corners android:radius="32dp"/>
        </shape>
    </item>
</selector>

